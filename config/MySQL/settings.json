{
   "_comment1": "'MVSF' contains Server Config Information for MVSF",
   "MVSF": {
      "nPort": "<PORT>",

      "_comment1": "If your server is using SSL, rename '_SSL' to 'SSL'",
      "_SSL": {
         "key":   "./ssl/server.key",
         "cert":  "./ssl/server.cert"
      },

      "IO": {
         "bGlobalNotify": true
      }
   },

   "SQL": {
      "type": "MYSQL",
      "config": {
         "connectionLimit":  10,
         "host":             "<MYSQLHOST>",
         "port":             "<MYSQLPORT>",
         "user":             "<MYSQLUSER>",
         "password":         "<MYSQLPASSWORD>",
         "database":         "<MYSQLDATABASE>",
         "multipleStatements": true
      }
   }
}
